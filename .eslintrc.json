{
  "extends": "eslint:recommended",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 6
  },
  "rules": {
    "no-extra-semi": 1,
    "no-unreachable": 1,
    "no-unused-vars": ["error", { "varsIgnorePattern": "^_", "args": "none"}],
    "semi": 0
  }
}
